class Solution(object):
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        
        dic = {}
        for i, j in enumerate(nums):
            r = target - j
            if r in dic:
                return(dic[r], i)
            else:
                dic[r] = i
        return
